{
  "id": "/marvin/frontend",
  "cpus": 0.1,
  "mem": 50,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mhausenblas/m-frontend:0.5.0",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0
        }
      ]
    }
  },
  "acceptedResourceRoles": [
    "slave_public"
  ]
}